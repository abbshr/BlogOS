<!DOCTYPE html>
<html>
	<head>
		<%- include head %>
	</head>
	<body>
		<%- include title %>
		<%- include adminnavbar %>
		<%- include flashinfo %>
		<label>应用信息列表</label><br>
		<ul>
		<% if (appInfo) { %>
			<% for (var i in appInfo) { %>
				<% if (i !== 'memory') { %>
				<li><%= appInfo[i] %></li>
				<% } %>
			<% } %>
		<% } %>
		</ul>
		<%- include foot %>
	</body>
</html>
